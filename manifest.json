{
    "name": "Teams Notifier",
    "version": "1.2.0",
    "description": "Microsoft Teams notifier extension.",
    "manifest_version": 2,
    "permissions": [
        "notifications",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "https://*.teams.microsoft.com/*"
    ],
    "background": {
        "scripts": ["util.js", "background.js"]
    },
    "browser_action": {
        "default_icon": {
            "16": "images/teams256.png",
            "32": "images/teams256.png",
            "48": "images/teams256.png",
            "128": "images/teams256.png"
        }
    },
    "icons": {
        "16": "images/teams256.png",
        "32": "images/teams256.png",
        "48": "images/teams256.png",
        "128": "images/teams256.png"
    }
}